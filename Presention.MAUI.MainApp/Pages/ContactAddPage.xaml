<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Presention.MAUI.MainApp.Pages.ContactAddPage"
             
             xmlns:viewmodels="clr-namespace:Presention.MAUI.MainApp.ViewModels"
             x:DataType="{x:Type viewmodels:ContactAddViewModel}"
             
             Title="Contact Add Page">
    <VerticalStackLayout>
        <Label 
            Text="Add New Contact"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Button Text="Contact List" Command="{Binding NavigateToListCommand}"/>
    </VerticalStackLayout>
</ContentPage>