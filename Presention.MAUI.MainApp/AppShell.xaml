<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Presention.MAUI.MainApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Presention.MAUI.MainApp"
    xmlns:pages="clr-namespace:Presention.MAUI.MainApp.Pages"
    Shell.FlyoutBehavior="Disabled"
    Shell.NavBarIsVisible="False"
    Title="Presention.MAUI.MainApp">

    <ShellContent Title="OverView" Route="ContactListPage" ContentTemplate="{DataTemplate pages:ContactListPage}"/>
    <ShellContent Title="Add Contact" Route="ContactAddPage" ContentTemplate="{DataTemplate pages:ContactAddPage}"/>
    <ShellContent Title="Update Contact" Route="ContactUpdatePage" ContentTemplate="{DataTemplate pages:ContactUpdatePage}"/>
    <ShellContent Title="Delet Contact" Route="ContactDeletPage" ContentTemplate="{DataTemplate pages:ContactDeletPage}"/>

</Shell>
