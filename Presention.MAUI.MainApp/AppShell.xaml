<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Presention.MAUI.MainApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Presention.MAUI.MainApp"
    xmlns:views="clr-namespace:Presention.MAUI.MainApp.Views"
    Shell.FlyoutBehavior="Disabled"

    Title="Presention.MAUI.MainApp">

    <ShellContent Title="List"  ContentTemplate="{DataTemplate ViewState:ListView}" Route="ListView" />
    <ShellContent Title="Add"  ContentTemplate="{DataTemplate ViewState:AddView}" Route="AddView"/>
    <ShellContent Title="Edit"  ContentTemplate="{DataTemplate ViewState:EditView}" Route="EditView"/>
    
</Shell>
