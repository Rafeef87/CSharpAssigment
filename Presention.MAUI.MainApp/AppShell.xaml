<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Presention.MAUI.MainApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Presention.MAUI.MainApp"
    xmlns:views="clr-namespace:Presention.MAUI.MainApp.Views"
    Shell.FlyoutBehavior="Disabled"
    Shell.NavBarIsVisible="True"
    Title="Presention.MAUI.MainApp">

    <ShellContent Title="List" ContentTemplate="{DataTemplate views:ContactListView}" Route="ContactListView"/>
    <ShellContent Title="Add"  ContentTemplate="{DataTemplate views:ContactAddView}" Route="ContactAddView"/>
    <ShellContent Title="Edit" ContentTemplate="{DataTemplate views:ContactEditView}" Route="ContactEditView"/>
    <ShellContent Title="Delete" ContentTemplate="{DataTemplate views:ContactDeleteView}" Route="ContactDeleteView"/>
    
</Shell>
